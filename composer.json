{
    "name": "backpack/demo",
    "description": "A Laravel + Backpack installation to show off most features.",
    "keywords": ["backpack", "laravel", "backpack for laravel", "admin panel", "crud"],
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0",
        "backpack/crud": "v6.x-dev as 5.99.99",
        "backpack/pro": "v6.x-dev as 1.99.99",
        "backpack/filemanager": "v6.x-dev as 2.99.99",
        "backpack/theme-coreuiv2": "dev-main",
        "backpack/theme-coreuiv4": "dev-main",
        "backpack/theme-tabler": "dev-main",
        "backpack/logmanager": "^4.0",
        "backpack/settings": "^3.0",
        "backpack/newscrud": "^4.0",
        "backpack/permissionmanager": "^6.0",
        "backpack/pagemanager": "^3.0",
        "backpack/menucrud": "^3.0",
        "spatie/laravel-translatable": "^5.0",
        "laravel/tinker": "^2.1",
        "spatie/laravel-ignition": "^1.0",
        "intervention/image": "^2.3",
        "backpack/revise-operation": "^1.0",
        "consoletvs/charts": "6.*",
        "laravel/legacy-factories": "^1.0",
        "mews/purifier": "^3.3",
        "backpack/backupmanager": "^3.0",
        "backpack/editable-columns": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "~1.4",
        "mockery/mockery": "1.5.*",
        "phpunit/phpunit" : "^9.0||^8.5||^7.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "^5.0.2|3.0.*|2.8.*",
        "laracasts/generators": "^2.0",
        "backpack/generators": "^3.1",
        "barryvdh/laravel-debugbar": "^3.2"
    },
    "repositories": {
        "backpack/theme-coreuiv2": {
            "type": "vcs",
            "url": "https://github.com/Laravel-Backpack/theme-coreuiv2.git"
        },
        "backpack/theme-coreuiv4": {
            "type": "vcs",
            "url": "https://github.com/Laravel-Backpack/theme-coreuiv4.git"
        },
        "backpack/theme-tabler": {
            "type": "vcs",
            "url": "https://github.com/Laravel-Backpack/theme-tabler.git"
        },
        "0": {
            "type": "composer",
            "url": "https://repo.backpackforlaravel.com/"
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate",
            "php artisan vendor:publish --provider=\"Backpack\\CRUD\\BackpackServiceProvider\" --tag=public --force"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "test": "vendor/bin/phpunit --testdox",
        "fresh": "php artisan key:generate --force && php artisan db:wipe --force && php artisan migrate --force && php artisan db:seed --force && php artisan backup:clean && php artisan cache:clear && php artisan view:clear && php artisan config:clear"
    },
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
